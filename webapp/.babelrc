{
    "presets": [
        ["env", {
            "targets": {
                "browsers": ["chrome >= 58", "safari >= 9"]
            },
            "useBuiltIns": true
        }],
        "stage-2"
    ],
    "plugins": [
        "syntax-dynamic-import",
        ["module-resolver", {
            "alias": {
                "@": ["./src"],
                "~": ["./"]
            }
        }]
    ] 
}